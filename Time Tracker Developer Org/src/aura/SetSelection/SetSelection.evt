<!--
	@ PURPOSE : THIS EVENT IS FIRED WHEN RECORD IS SELECTED FROM DROP DOWN LIST IN TYPEAHEAD FEATURE.
	@ NAME	  : SetSelection.evt
-->
<aura:event type="APPLICATION" description="Event template" >
    
	<!-- HOLDS THE SELECTED RECORD'S LABEL, VALUE, TARGET COMPONENT'S AURA ID, AND THE NAME OF THE ATTRIBUTE THAT HOLDS LIST OF VALUES. -->
    <aura:attribute name="selectedRecord" type="Object" default="{}"/>
    
</aura:event>